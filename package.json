{
  "name": "npm-lib-template",
  "version": "0.0.1",
  "description": "npm-lib-template",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "esnext": "lib/esnext/index.js",
  "typings": "lib/types/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "start": "tsc -w",
    "build": "run-p \"build:*\"",
    "build:cjs": "tsc -m commonjs --outDir lib/cjs",
    "build:esm": "tsc -m es2015 --outDir lib/esm",
    "build:esnext": "tsc -m esnext --outDir lib/esnext",
    "build:types": "tsc -m commonjs --declaration --emitDeclarationOnly --outDir lib/types",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukasbach/npm-lib-template.git"
  },
  "author": "Lukas Bach <npm@lukasbach.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lukasbach/npm-lib-template/issues"
  },
  "homepage": "https://github.com/lukasbach/npm-lib-template#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
